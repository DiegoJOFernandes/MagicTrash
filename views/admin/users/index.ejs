<%- include ('../../partials/header.ejs') %>

  <%- include ('../../partials/navbar.ejs') %>
  <div class="container">
    <hr>
    <h2>Usuários</h2>
    <a class="btn-themex" href="/admin/users/create">Criar novo usuário</a>
    <hr>
    <table class="table table-boardred">
      <thead>
        <tr>
          <th>Id</th>
          <th>Email</th>

        </tr>
      </thead>
      <tbody>
        <% users.forEach(user => { %>
          <tr>
            <td><%= user.id %></td>
            <td><%= user.email %></td>
          </tr>          
        <% }) %>
      </tbody>
    </table>
<%- include ('../../partials/footer.ejs') %>